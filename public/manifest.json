{
  "manifest_version": 3,
  "name": "DarkMoodle",
  "version": "1.0",
  "description": "Dites adieu à la fatigue oculaire, et bonjour à la concentration ultime.",
  "content_scripts": [
    {
      "matches": ["https://moodle.cesi.fr/*"],
      "css": ["my-styles.css"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon_16.jpg",
      "48": "icons/icon_48.jpg",
      "128": "icons/icon_128.jpg"
    }
  }
}